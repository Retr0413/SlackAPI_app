{
    "name": "My Dev Container",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "web",
    "workspaceFolder": "/workspace",
    "extensions": [
        "ms-python.python",
        "ms-azuretools.vscode-docker"
    ],
    "settings": {
        "python.pythonPath": "/usr/local/bin/python"
    },
    "postCreateCommand": "pip install -r requirements.txt",
    "remoteUser": "root"
}
